{
  "name": "ppa1",
  "version": "1.0.0",
  "description": "Professional Practice Assignment #1 for Unit Testing and T/BDD",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erinyoo/ppa1.git"
  },
  "author": "Erin Yoo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/erinyoo/ppa1/issues"
  },
  "homepage": "https://github.com/erinyoo/ppa1#readme",
  "devDependencies": {
    "jest": "^24.9.0"
  },
  "dependencies": {
    "prompts": "^2.2.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "functions/bmi.js",
      "functions/emailverifier.js",
      "functions/retirement.js",
      "functions/shortestdistance.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 85,
        "branches": 85,
        "functions": 85,
        "lines": 85
      }
    }
  }
}
